const liaisonLinks = {
                "Anastasi, Kathryn (Katy)": "<a href=\"https://library.clark.edu/staff/anastasi-kathryn-katy\">Anastasi, Kathryn (Katy)</a>",
                "Berg, Andrea": "<a href=\"https://library.clark.edu/staff/berg-andrea\">Berg, Andrea</a>",
